[smlsharp]
name=SML# for Fedora - $basearch
baseurl=https://smlsharp.github.io/repos/fedora/rawhide/$basearch/
gpgcheck=1
repo_gpgcheck=1
enabled=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-smlsharp

[smlsharp-src]
name=SML# for Fedora - Source
baseurl=https://smlsharp.github.io/repos/fedora/rawhide/source/
gpgcheck=1
repo_gpgcheck=1
enabled=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-smlsharp
