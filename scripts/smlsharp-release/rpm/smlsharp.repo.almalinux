[smlsharp]
name=SML# for AlmaLinux $releasever - $basearch
baseurl=https://smlsharp.github.io/repos/almalinux/$releasever/$basearch/
gpgcheck=1
repo_gpgcheck=1
enabled=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-smlsharp

[smlsharp-src]
name=SML# for AlmaLinux $releasever - Source
baseurl=https://smlsharp.github.io/repos/almalinux/$releasever/source/
gpgcheck=1
repo_gpgcheck=1
enabled=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-smlsharp
