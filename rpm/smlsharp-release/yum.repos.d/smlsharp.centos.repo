[smlsharp]
name=SML# for CentOS $releasever - $basearch
baseurl=https://github.com/smlsharp/repos/raw/main/centos/$releasever/$basearch/
gpgcheck=1
repo_gpgcheck=1
enabled=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-smlsharp

[smlsharp-src]
name=SML# for CentOS $releasever - Source
baseurl=https://github.com/smlsharp/repos/raw/main/centos/$releasever/source/
gpgcheck=1
repo_gpgcheck=1
enabled=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-smlsharp
